vsim work.program_counter(table)
# vsim work.program_counter(table) 
# Start time: 09:56:15 on Nov 16,2018
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading ieee.std_logic_arith(body)
# Loading ieee.std_logic_unsigned(body)
# Loading ieee.numeric_std(body)
# Loading work.program_counter(table)
wave create -driver freeze -pattern clock -initialvalue 0 -period 100ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/program_counter/clk
# Editable Wave items are not compatible with the wave window's standard undo-redo.  Standard undo-redo has been disabled.
wave create -driver freeze -pattern constant -value 0000000000000010 -range 15 0 -starttime 0ps -endtime 1000ps sim:/program_counter/IR
wave create -driver freeze -pattern constant -value 1000000000000000 -range 15 0 -starttime 0ps -endtime 1000ps sim:/program_counter/AD
wave create -driver freeze -pattern constant -value 00 -range 1 0 -starttime 0ps -endtime 1000ps sim:/program_counter/OP
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 1000ps sim:/program_counter/R
run -all
wave modify -driver freeze -pattern constant -value 01 -range 1 0 -starttime 0ps -endtime 1000ps Edit:/program_counter/OP
run -all
run -all
# 
# Internal Error: In tclprim_ApplyStimulus for /program_counter/clk "Time value must be greater than "now": 1000 ps
# ** Error: (vsim-4004) The -cancel option requires a time period argument.
